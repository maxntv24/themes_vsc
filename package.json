{
  "name": "srs-highlighter",
  "displayName": "SRS Highlighter",
  "description": "Syntax highlighting for .srs files",
  "version": "0.0.1",
  "publisher": "maxntv",
  "engines": {
    "vscode": "^1.82.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "srs",
        "aliases": [
          "Scan Result Log",
          "srs"
        ],
        "extensions": [
          ".srs",
          ".log"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "srs",
        "scopeName": "source.srs",
        "path": "./syntaxes/srs.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "SRS Theme Github",
        "uiTheme": "vs-dark",
        "path": "./themes/srs-color-theme.json"
      },
      {
        "label": "SRS Default",
        "uiTheme": "vs-dark",
        "path": "./themes/default_vscode.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxntv24/themes_vsc"
  }
}