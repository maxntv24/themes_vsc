{
  "name": "rsm-highlighter",
  "displayName": "RSM Highlighter",
  "description": "Syntax highlighting for .rsm files",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.82.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "rsm",
        "aliases": [
          "RSM Log",
          "rsm"
        ],
        "extensions": [
          ".rsm",
          ".log"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rsm",
        "scopeName": "source.rsm",
        "path": "./syntaxes/rsm.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "RSM Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/rsm-color-theme.json"
      }
    ]
  },
    "devDependencies": {
      "@changesets/changelog-github": "^0.4.1",
      "@changesets/cli": "^2.17.0",
      "@primer/primitives": "7.10.0",
      "chroma-js": "^2.1.2",
      "color": "^3.1.2",
      "nodemon": "^2.0.3",
      "vsce": "^1.100.1"
    }
}
